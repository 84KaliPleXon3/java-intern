{
  "name": "intern",
  "version": "5.0.0-pre",
  "description": "Intern. A next-generation code testing stack for JavaScript.",
  "homepage": "http://theintern.io/",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/theintern/intern/issues",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "bdd",
    "browserstack",
    "ci",
    "code coverage",
    "continuous integration",
    "javascript",
    "sauce labs",
    "selenium",
    "tdd",
    "test",
    "testing",
    "unit",
    "webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/theintern/intern.git"
  },
  "bin": {
    "intern": "./bin/intern.js"
  },
  "scripts": {
    "clean": "ts-node scripts/clean.ts",
    "build:api": "ts-node scripts/api.ts",
    "build": "ts-node scripts/build.ts",
    "intern": "ts-node -r tsconfig-paths/register src/bin/intern.ts",
    "devpack": "ts-node scripts/build.ts --pack",
    "lint": "eslint src --ext .ts",
    "release": "cross-env INTERN_BUILD=release ts-node scripts/release.ts",
    "start": "ts-node scripts/test.ts --serve-only",
    "test": "ts-node scripts/test.ts",
    "test:chrome": "ts-node scripts/test.ts --config @chrome",
    "test:edge": "ts-node scripts/test.ts --config @edge",
    "test:firefox": "ts-node scripts/test.ts --config @firefox",
    "test:safari": "ts-node scripts/test.ts --config @safari",
    "watch:tests": "ts-node scripts/test.ts --watch",
    "watch": "ts-node scripts/build.ts --watch",
    "ci": "ts-node scripts/test.ts --config @ci"
  },
  "dependencies": {
    "@types/benchmark": "~1.0.31",
    "@types/chai": "~4.2.7",
    "@types/charm": "~1.0.1",
    "@types/express": "~4.17.0",
    "@types/istanbul-lib-coverage": "~2.0.1",
    "@types/istanbul-lib-instrument": "~1.7.3",
    "@types/istanbul-lib-report": "~3.0.0",
    "@types/istanbul-lib-source-maps": "~1.2.2",
    "@types/istanbul-reports": "~1.1.1",
    "@types/ws": "~7.2.4",
    "axios": "~0.19.0",
    "benchmark": "~2.1.4",
    "body-parser": "~1.19.0",
    "bufferutil": "~4.0.1",
    "chai": "~4.2.0",
    "charm": "~1.0.2",
    "chokidar": "~3.3.0",
    "command-exists": "~1.2.8",
    "commander": "~4.0.1",
    "concurrent": "~0.3.2",
    "decompress": "~4.2.0",
    "diff": "~4.0.1",
    "express": "~4.17.1",
    "glob": "~7.1.4",
    "http-errors": "~1.7.2",
    "istanbul-lib-coverage": "~3.0.0",
    "istanbul-lib-hook": "~3.0.0",
    "istanbul-lib-instrument": "~4.0.1",
    "istanbul-lib-report": "~3.0.0",
    "istanbul-lib-source-maps": "~4.0.0",
    "istanbul-reports": "~3.0.0",
    "jszip": "~3.2.2",
    "lodash": "~4.17.15",
    "mime-types": "~2.1.24",
    "minimatch": "~3.0.4",
    "open": "~7.0.0",
    "platform": "~1.3.5",
    "qs": "~6.9.1",
    "resolve": "~1.13.1",
    "shell-quote": "~1.7.2",
    "source-map": "~0.6.1",
    "ts-node": "~8.6.2",
    "tslib": "~1.10.0",
    "utf-8-validate": "~5.0.2",
    "ws": "~7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~8.3.5",
    "@commitlint/config-conventional": "~8.3.4",
    "@dojo/loader": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/command-exists": "^1.2.0",
    "@types/decompress": "^4.2.3",
    "@types/diff": "^4.0.2",
    "@types/glob": "^7.1.1",
    "@types/grunt": "^0.4.25",
    "@types/http-errors": "^1.6.1",
    "@types/istanbul-lib-hook": "^2.0.0",
    "@types/jsdom": "^12.2.3",
    "@types/jszip": "~3.1.6",
    "@types/lodash": "^4.14.141",
    "@types/mime-types": "^2.1.0",
    "@types/moxios": "~0.4.9",
    "@types/node": "^12.12.17",
    "@types/parse5": "^5.0.0",
    "@types/platform": "^1.3.2",
    "@types/qs": "~6.9.0",
    "@types/resolve": "^0.0.8",
    "@types/semver": "^6.2.0",
    "@types/shell-quote": "^1.6.1",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.0.12",
    "@types/systemjs": "^0.20.7",
    "@types/webpack": "^4.4.32",
    "@typescript-eslint/eslint-plugin": "~2.14.0",
    "@typescript-eslint/parser": "~2.14.0",
    "ajv": "^6.10.0",
    "canvas": "~2.6.1",
    "concurrently": "^5.0.1",
    "core-js": "^3.5.0",
    "core-js-pure": "^3.5.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.9.0",
    "execa": "~4.0.0",
    "husky": "~4.0.0",
    "jsdom": "^15.1.1",
    "lint-staged": "~10.0.7",
    "moxios": "~0.4.0",
    "prettier": "^1.17.1",
    "rewiremock": "~3.13.9",
    "semver": "^6.3.0",
    "shelljs": "~0.8.4",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "style-loader": "^1.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.0.2",
    "tsconfig-paths": "~3.9.0",
    "tsconfig-paths-webpack-plugin": "~3.2.0",
    "typedoc": "^0.15.4",
    "typescript": "~3.7.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{json,css}": [
      "prettier --write"
    ],
    "tests/**/*.{json,css}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.ts": [
      "npm run build:api"
    ],
    "*.md": [
      "prettier --write"
    ],
    "{intern,tsconfig}.json": [
      "prettier --write"
    ]
  }
}
