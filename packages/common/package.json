{
  "name": "@theintern/common",
  "version": "0.2.4-pre",
  "description": "Common modules used by @theintern projects",
  "homepage": "http://theintern.io/",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/theintern/intern/issues",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "javascript",
    "test",
    "testing",
    "intern",
    "leadfoot",
    "digdug"
  ],
  "files": [
    "dist/*",
    "amd/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/theintern/intern.git"
  },
  "scripts": {
    "build": "tsc -b && tsc -b tsconfig.amd.json",
    "clean": "shx rm -rf dist _tests amd *.tsbuildinfo",
    "pretest": "tsc -b tests && webpack --config tests/webpack.config.ts",
    "test": "intern",
    "test:chrome": "intern --config @chrome",
    "test:edge": "intern --config @edge",
    "test:firefox": "intern --config @firefox",
    "test:safari": "intern --config @safari"
  },
  "dependencies": {
    "axios": "~0.19.0",
    "tslib": "~1.9.3"
  },
  "devDependencies": {
    "@theintern-dev/tsconfig": "workspace:*",
    "@theintern/core": "workspace:*",
    "@types/copy-webpack-plugin": "^5.0.2",
    "@types/glob": "^7.1.3",
    "@types/moxios": "^0.4.9",
    "@types/qs": "^6.9.4",
    "@types/webpack": "^4.41.21",
    "@types/webpack-node-externals": "^1.7.1",
    "copy-webpack-plugin": "^5.1.1",
    "glob": "^7.1.6",
    "istanbul-instrumenter-loader": "^3.0.1",
    "moxios": "^0.4.0",
    "qs": "^6.9.4",
    "ts-loader": "^6.2.2",
    "umd-compat-loader": "^2.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  }
}
